#
#	Logrotate fragment for squid.
#
/var/log/squid-ssl/*.log {
	daily
	compress
	delaycompress
	rotate 2
	missingok
	nocreate
	sharedscripts
	prerotate
		test ! -x /usr/sbin/sarg-reports || /usr/sbin/sarg-reports
	endscript
	postrotate
		test ! -e /var/run/squid-ssl.pid || test ! -x /usr/sbin/squid-ssl || /usr/sbin/squid-ssl -k rotate
	endscript
}
